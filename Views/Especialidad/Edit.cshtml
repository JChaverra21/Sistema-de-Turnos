@model Turno.Models.Especialidad

<h4>Especialidad</h4>

<div class="row">
    <form class="col s12" asp-action="Edit">
        <div asp-validation-summary="ModelOnly"></div>
        <input type="hidden" asp-for="IdEspecialidad" />
        <div class="input-field col s12">
            <i class="material-icons prefix">description</i>
            <input asp-for="Descripcion" type="text" class="validate">
            <label asp-for="Descripcion"></label>
            <span asp-validation-for="Descripcion"></span>
        </div>
        <div class="row">
            <div class="col s6 m6">
                <button class="btn waves-effect waves-light cyan darken-4" type="submit" name="action">Grabar
                    <i class="material-icons right">send</i>
                </button>
            </div>

            <div class="col s6 m6">
                <a class="waves-effect waves-light btn grey darken-4" asp-action="Index"><i
                        class="material-icons left">undo</i>Volver</a>
            </div>
        </div>
        @* <div>
            <label asp-for="Descripcion"></label>
            <input asp-for="Descripcion" />
            <span asp-validation-for="Descripcion"></span>
        </div>
        <div>
            <input type="submit" value="Grabar" />
        </div> *@
    </form>
</div>
